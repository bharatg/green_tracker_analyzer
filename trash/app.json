{
  "name": "Green Tracker Analyzer",
  "description": "Pyhon app",
  "image": "heroku/python",
  "repository": "https://github.com/green-cani/green-cani-analyzer",
  "keywords": ["python"],
  "addons": [ "heroku-postgresql" ],
  "env": {
    "SECRET_KEY": {
      "description": "The secret key for the Django application.",
      "generator": "secret"
    }
  },
  "environments": {
    "test": {
      "scripts": {
        "test-setup": "python manage.py collectstatic --noinput",
        "test": "python manage.py test"
      }
    }
  }
}
